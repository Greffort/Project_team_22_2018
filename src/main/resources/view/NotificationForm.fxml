<VBox prefHeight="285.0" prefWidth="295.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.team22.project_team_22_2018.client.view.fxcontroller.NotificationFormController">
